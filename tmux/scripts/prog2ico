    hours                           ) echo -n "Û∞îõ" ;;
    swipl                           ) echo -n "Óû°" ;;
    man|glow*                       ) echo -n "ÔÄ≠" ;;
    rizin                           ) echo -n "Û±ÅÜ" ;;
    gdb|rr                          ) echo -n "ÔÜà" ;;
    bat                             ) echo -n "Û∞àÆ" ;;
    *python*|pip[23]                ) echo -n "Óàµ" ;;
    just|meson|cmake|make|ninja     ) echo -n "ÔÇÖ" ;;
    kubectl|k9s                     ) echo -n "Û±Éæ" ;;
    docker*                         ) echo -n "Ôåà" ;;
