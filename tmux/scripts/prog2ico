    bat                             ) echo -n "Û∞àÆ" ;;
    brew                            ) echo -n "ÓüΩ" ;;
    cargo|rust                      ) echo -n "Óû®" ;;
    cmake|just|make|meson|ninja     ) echo -n "ÔÇÖ" ;;
    crush                           ) echo -n "Ó¨Ö" ;;
    delta                           ) echo -n "‚àÜ" ;;
    docker*                         ) echo -n "Ôåà" ;;
    gdb|rr                          ) echo -n "ÔÜà" ;;
    gh                              ) echo -n "ÔÇõ" ;;
    ghc*|stack                      ) echo -n "Óòü" ;;
    git*                            ) echo -n "Ôáì" ;;
